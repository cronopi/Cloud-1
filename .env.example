# Variables de configuración para Cloud-1
# INSTRUCCIONES:
# 1. Copiar este archivo: cp .env.example .env
# 2. Editar .env con tus credenciales reales
# 3. NUNCA subir .env a Git (ya está en .gitignore)

# MySQL Configuration
MYSQL_DATABASE=wordpress
MYSQL_USER=wpuser
MYSQL_PASSWORD=change_me_secure_password
MYSQL_ROOT_PASSWORD=change_me_root_password

# WordPress Configuration
WORDPRESS_DB_HOST=mysql
WORDPRESS_DB_NAME=wordpress
WORDPRESS_DB_USER=wpuser
WORDPRESS_DB_PASSWORD=change_me_secure_password

# Opcional: Datos de WordPress
WORDPRESS_TABLE_PREFIX=wp_
WORDPRESS_DEBUG=false
